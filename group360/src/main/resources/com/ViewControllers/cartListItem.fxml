<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<HBox fx:id="itemOneHbox" prefHeight="40.0" prefWidth="384.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Text fx:id="itemNameLabel" strokeType="OUTSIDE" strokeWidth="0.0" text="String">
         <HBox.margin>
            <Insets left="11.0" top="8.0" />
         </HBox.margin>
      </Text>
      <Button id="itemOneQMinus" alignment="CENTER" mnemonicParsing="false" onAction="#QtyDown" text="-">
         <font>
            <Font size="14.0" />
         </font>
         <HBox.margin>
            <Insets left="110.0" top="4.0" />
         </HBox.margin>
      </Button>
      <Text fx:id="itemOneQText" strokeType="OUTSIDE" strokeWidth="0.0" text="int">
         <HBox.margin>
            <Insets left="20.0" top="8.0" />
         </HBox.margin>
      </Text>
      <Button id="itemOneQPlus" alignment="CENTER" mnemonicParsing="false" onAction="#QtyUp" text="+">
         <font>
            <Font size="14.0" />
         </font>
         <HBox.margin>
            <Insets left="20.0" top="4.0" />
         </HBox.margin>
      </Button>
      <Text fx:id="itemOnePText" strokeType="OUTSIDE" strokeWidth="0.0" text="float">
         <HBox.margin>
            <Insets left="90.0" top="8.0" />
         </HBox.margin>
      </Text>
   </children>
</HBox>
